<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<widget xmlns="http://www.w3.org/ns/widgets" id="de.drinkndrive.drive"
	version="1.0.7">
	<name>Drive</name>
	<description>
		Drive - The essential share driving
	</description>
	<author email="info@drinkndrive.de" href="http://www.drinkndrive.de">
		drinkndrive team
	</author>
	<content src="cordova.html" />
	<access origin="*" />
	<preference name="SplashScreen" value="screen" />
	<preference name="SplashScreenDelay" value="3000" />
	<preference name="Fullscreen" value="false" />
	<preference name="DisallowOverscroll" value="true" />
	<preference name="WebViewBounce" value="true" />
	<preference name="Orientation" value="portrait" />
	<preference name="StatusBarOverlaysWebView" value="true" />
	<preference name="StatusBarStyle" value="lightcontent" />
	<preference name="BackupWebStorage" value="local" />
	<plugin name="Geolocation" value="CDVLocation" />

	<platform name="android">
		<preference name="android-minSdkVersion" value="14" />
		<preference name="android-targetSdkVersion" value="19" />
		<icon density="ldpi" src="res/icon/android/icon-ldpi.png" />
		<icon density="mdpi" src="res/icon/android/icon-mdpi.png" />
		<icon density="hdpi" src="res/icon/android/icon-hdpi.png" />
		<icon density="xhdpi" src="res/icon/android/icon-xhdpi.png" />
	</platform>


	<platform name="ios">
		<preference name="target-device" value="handset" />
		<preference name="deployment-target" value="7.0" />
		<!-- iOS 7.0+ -->
		<!-- iPhone / iPod Touch -->
		<icon height="60" src="res/icon/ios/AppIcon60x60.png" width="60" />
		<icon height="120" src="res/icon/ios/AppIcon60x60@2x.png" width="120" />
		<icon height="180" src="res/icon/ios/AppIcon60x60@3x.png" width="180" />
		<!-- iPad -->
		<icon height="76" src="res/icon/ios/AppIcon76x76.png" width="76" />
		<icon height="152" src="res/icon/ios/AppIcon76x76@2x.png" width="152" />
		<!-- iOS 6.1 -->
		<!-- Spotlight Icon -->
		<icon height="40" src="res/icon/ios/AppIcon40x40.png" width="40" />
		<icon height="80" src="res/icon/ios/AppIcon40x40@2x.png" width="80" />
		<icon height="120" src="res/icon/ios/AppIcon40x40@3x.png" width="120" />
		<!-- iPhone Spotlight and Settings Icon -->
		<icon height="29" src="res/icon/ios/AppIcon29x29.png" width="29" />
		<icon height="58" src="res/icon/ios/AppIcon29x29@2x.png" width="58" />
		<icon height="87" src="res/icon/ios/AppIcon29x29@3x.png" width="87" />

	</platform>
	<feature name="UniversalAnalytics">
		<param name="android-package"
			value="com.danielcwilson.plugins.analytics.UniversalAnalyticsPlugin" />
		<param name="ios-package" value="UniversalAnalyticsPlugin" />
	</feature>
	<feature name="Google Universal Analytics Plugin">
		<param name="id" value="com.danielcwilson.plugins.googleanalytics" />
	</feature>
	<feature name="FacebookConnectPlugin">
		<param name="ios-package" value="FacebookConnectPlugin" />
		<param name="onload" value="true" />
	</feature>
	<feature name="Facebook Connect">
		<param name="id" value="com.phonegap.plugins.facebookconnect" />
	</feature>
	<feature name="DatePickerPlugin">
		<param name="android-package" value="com.plugin.datepicker.DatePickerPlugin" />
	</feature>
	<feature name="DatePicker">
		<param name="ios-package" value="DatePicker" />
		<param name="id" value="com.plugin.datepicker" />
	</feature>
	<feature name="YoikScreenOrientation">
		<param name="android-package"
			value="net.yoik.cordova.plugins.screenorientation.YoikScreenOrientation" />
		<param name="ios-package" value="YoikScreenOrientation" />
	</feature>
	<feature name="Screen Orientation">
		<param name="id" value="net.yoik.cordova.plugins.screenorientation" />
	</feature>
	<feature name="EmailComposer">
		<param name="android-package"
			value="de.appplant.cordova.plugin.emailcomposer.EmailComposer" />
		<param name="ios-package" value="APPEmailComposer" />
		<param name="id" value="de.appplant.cordova.plugin.email-composer" />
	</feature>
	<feature name="Device">
		<param name="android-package" value="org.apache.cordova.device.Device" />
		<param name="ios-package" value="CDVDevice" />
	</feature>
	<feature name="LocalNotification">
		<param name="android-package"
			value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
		<param name="ios-package" value="APPLocalNotification" />
		<param name="onload" value="true" />
		<param name="id" value="de.appplant.cordova.plugin.local-notification" />
	</feature>
	<feature name="SocialSharing">
		<param name="android-package" value="nl.xservices.plugins.SocialSharing" />
		<param name="ios-package" value="SocialSharing" />
		<param name="onload" value="true" />
		<param name="id" value="nl.x-services.plugins.socialsharing" />
	</feature>
	<feature name="Notification">
		<param name="android-package" value="org.apache.cordova.dialogs.Notification" />
		<param name="ios-package" value="CDVNotification" />


	<param name="id" value="org.apache.cordova.dialogs" /><param name="url" value="https://github.com/apache/cordova-plugin-dialogs" /></feature>
	<feature name="InAppBrowser">
		<param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
		<param name="ios-package" value="CDVInAppBrowser" />

		<param name="id" value="org.apache.cordova.inappbrowser" />
	</feature>
	<feature name="NetworkStatus">
		<param name="android-package"
			value="org.apache.cordova.networkinformation.NetworkManager" />
		<param name="ios-package" value="CDVConnection" />
	</feature>
	<feature name="Network Information">

		<param name="id" value="org.apache.cordova.network-information" />
	</feature>
	<feature name="SplashScreen">
		<param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
		<param name="ios-package" value="CDVSplashScreen" />
		<param name="onload" value="true" />
	</feature>
	<feature name="Splashscreen">

		<param name="id" value="org.apache.cordova.splashscreen" />
	</feature>
	<feature name="StatusBar">
		<param name="android-package" value="org.apache.cordova.statusbar.StatusBar" />
		<param name="onload" value="true" />
		<param name="ios-package" value="CDVStatusBar" />

		<param name="id" value="org.apache.cordova.statusbar" />
	</feature>
	<feature name="Geolocation">
		<param name="ios-package" value="CDVLocation" />
	</feature>
	<feature name="BackgroundGeoLocation">
		<param name="android-package"
			value="com.tenforwardconsulting.cordova.bgloc.BackgroundGpsPlugin" />
		<param name="ios-package" value="CDVBackgroundGeoLocation" />
	</feature>
	<feature name="CDVBackgroundGeoLocation">
		<param name="id" value="org.transistorsoft.cordova.background-geolocation" />
	</feature>
	<feature name="HttpRequest">
		<param name="android-package" value="plugin.http.request.HttpRequest" />
		<param name="ios-package" value="HttpRequest" />
	</feature>
	<preference name="APP_ID" value="PLEASE_DEFINE" />
	<preference name="APP_NAME" value="PLEASE_DEFINE" />

	<feature name="GoogleMaps">
		<param name="android-package" value="plugin.google.maps.GoogleMaps" />
	</feature>
	<feature name="External">
		<param name="android-package" value="plugin.google.maps.External" />
	</feature>
	<feature name="Geocoder">
		<param name="android-package" value="plugin.google.maps.MyGeocoder" />
	</feature>
	<preference name="API_KEY_FOR_ANDROID" value="PLEASE_DEFINE" />

	<feature name="GoogleMaps">
		<param name="ios-package" value="GoogleMaps" />
	</feature>
	<feature name="External">
		<param name="ios-package" value="External" />
	</feature>
	<feature name="Geocoder">
		<param name="ios-package" value="Geocoder" />
	</feature>
	<feature name="phonegap-http-request">
		<param name="id" value="plugin.http.request" />
	</feature>
	<feature name="AppVersion">
		<param name="android-package" value="uk.co.whiteoctober.cordova.AppVersion" />
		<param name="ios-package" value="AppVersion" />
		<param name="id" value="uk.co.whiteoctober.cordova.appversion" />
	</feature>
	<feature name="Adjust">
		<param name="android-package" value="com.adjust.sdk.AdjustCordova" />
		<param name="onload" value="true" />
		<param name="ios-package" value="AdjustCordova" />
		<param name="id" value="com.adjust.sdk" />
		<param name="url" value="https://github.com/adjust/cordova_sdk" />
	</feature>
	<feature name="Camera">
		<param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
		<param name="ios-package" value="CDVCamera" />
		<param name="id" value="org.apache.cordova.camera" />
	</feature>
	<feature name="Mixpanel">
		<param name="ios-package" value="CDVMixpanel" />
		<param name="id" value="org.poetic.mixpanel" />
		<param name="url" value="https://github.com/poetic/cordova-mixpanel" />
	</feature>
	<preference name="API_KEY_FOR_IOS" value="PLEASE_DEFINE" />
</widget>
